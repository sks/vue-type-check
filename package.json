{
    "name": "vue-type-check-hapyharshit",
    "version": "1.2.0",
    "description": "a type checker for typescript written Vue components",
    "main": "dist/index.js",
    "bin": {
        "vue-type-check-hapyharshit": "dist/cli.js",
        "vtch": "dist/cli.js",
        "vtch-get-exclude": "dist/exclude.js"
    },
    "scripts": {
        "compile": "tsc -p .",
        "watch": "tsc --watch",
        "typings": "tsc -d --emitDeclarationOnly --declarationDir typings",
        "start": "node dist/index.js",
        "test": "node tests/component.spec.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:hapyharshit/vue-type-check.git"
    },
    "keywords": [
        "vue",
        "typescript",
        "types",
        "vetur"
    ],
    "author": "hapyharshit@gmail.com",
    "license": "MIT",
    "files": [
        "dist",
        "typings"
    ],
    "typings": "typings/index.d.ts",
    "dependencies": {
        "@types/chalk": "^2.2.0",
        "@types/eslint": "^7.2.0",
        "@types/glob": "^7.1.3",
        "@types/minimist": "^1.2.0",
        "@types/prettier": "^2.0.2",
        "@types/progress": "^2.0.3",
        "chalk": "^4.1.0",
        "glob": "^7.1.6",
        "minimist": "^1.2.5",
        "progress": "^2.0.3",
        "vscode-languageserver": "^6.1.1",
        "vue-language-server": "^0.0.67"
    },
    "peerDependencies": {
        "typescript": "^3.9.6"
    },
    "devDependencies": {
        "typescript": "^3.9.6",
        "vue": "^2.6.10"
    }
}
